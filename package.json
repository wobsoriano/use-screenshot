{
  "name": "use-screenshot",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Vue composable to take screenshots of a given web page, tab, window, or the user's entire screen.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/pkg-name.git"
  },
  "bugs": "https://github.com/wobsoriano/pkg-name/issues",
  "keywords": [
    "vue",
    "composable",
    "hook"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "nr build-fast --dts-resolve",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@xata.io/screenshot": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "jsdom": "^19.0.0",
    "pnpm": "^7.2.1",
    "tsup": "6.0.1",
    "typescript": "^4.7.4",
    "vue": "^3.2.37"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
